addofnum(int a , int b , int c){
   if(a==b){
     return a+b;
   }
   if(b<a){
    return a-b;
  }
  if(b<c){
    return a*c;
  }
  if(c>a){
    return 9;
  }
}

void main() {
  int a= 12;
  int b = 23;
  int c = 49;
  var total = addofnum(a,b,c);
  print("Value of a is : $a");
  print("Value of b is : $b");
  print("Value of c is : $c");
  print("Value of function is : $total");  
}
